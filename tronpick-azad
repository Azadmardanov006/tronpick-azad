								heading: 'Error!',
								text: obj.mes,
								showHideTransition: 'slide',
								position: 'top-right',
								icon: 'error'
							});
						} else {
							var interval = setInterval(function(){
								$('#bonus_faucet .first_digit').text(Math.floor(Math.random() * 10));
								$('#bonus_faucet .second_digit').text(Math.floor(Math.random() * 10));
								$('#bonus_faucet .third_digit').text(Math.floor(Math.random() * 10));
								$('#bonus_faucet .fourth_digit').text(Math.floor(Math.random() * 10));
								$('#bonus_faucet .fifth_digit').text(Math.floor(Math.random() * 10));
							}, 50);
							setTimeout(function() {
								clearInterval(interval);
								$('#bonus_faucet .first_digit').text(obj.num[0]);
								$('#bonus_faucet .second_digit').text(obj.num[1]);
								$('#bonus_faucet .third_digit').text(obj.num[2]);
								$('#bonus_faucet .fourth_digit').text(obj.num[3]);
								$('#bonus_faucet .fifth_digit').text(obj.num[4]);
							}, 1000);	
							setTimeout(function() {
								$("#process_claim_bonus_faucet").attr("disabled", false);
								$("#process_claim_bonus_faucet img").hide();
								$("#process_claim_bonus_faucet span").show();	
								//$("#process_claim_bonus_faucet").text(btn_label);
								$('.user_balance').text(parseFloat((parseInt(obj.balance)) / 1000000).toFixed(6));
								$('#free_spins').text(parseInt(obj.free_spins));
								$.toast({
									heading: 'Success!',
									text: obj.mes,
									showHideTransition: 'slide',
									position: 'top-right',
									icon: 'success'
								});
							}, 2000);	
						}
					}
				});
			}
			
			function change_faucet_type() {
