<!-- hcaptcha -->
<script>
	var hCaptchaBox;
	function setup_hCaptcha() {
		var params = {
			"sitekey": '8628f615-3f07-4c0b-a889-c8a20544e46e',
		};
		hCaptchaBox = hcaptcha.render("hCaptchaBox", params);
	}
	function reset_hCaptcha() {
		hcaptcha.reset(hCaptchaBox);
	}
</script>
<script src="https://js.hcaptcha.com/1/api.js?onload=setup_hCaptcha&amp;recaptchacompat=off" async="" defer=""></script>
