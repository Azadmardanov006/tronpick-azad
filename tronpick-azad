			function load_deposit_transactions() {
								var last_deposit = parseInt(1745694043);
				$.ajax({
					url: 'https://apilist.tronscanapi.com/api/transaction?sort=-timestamp&count=true&limit=1&start=0&address=TBCZkoQRaAv9gQ2Aw4KscaKN6EhsZ4rQyX',
					beforeSend: function(xhr) {},
					success: function(data){
						var transactions = data.data;
						if(transactions.length > 0) {
							var timestamp = parseInt(parseInt(transactions[0].timestamp) / 1000);
							//console.log(timestamp);
							if(timestamp > last_deposit) {
								$.ajax({
									url: "https://tronpick.io/load.php",
									type: "POST",
									data: "data=deposit_transactions",
									error: function(){},
									success: function(out){
										obj = JSON.parse(out);
										if(obj.ret == 1) {
											$('.user_balance').text(parseFloat((parseInt(obj.balance)) / 1000000).toFixed(6));
											$.toast({
												heading: 'Success!',
												text: obj.mes,
												showHideTransition: 'slide',
												position: 'top-right',
												icon: 'success'
											});
										}
									}
								});
							}
						}
					}
				});
							}
			
			function process_complete_confirmed_request() {
				$.ajax({
					url: "https://tronpick.io/process.php",
					type: "POST",
					data: "action=complete_confirmed_request",
					error: function(){},
					success: function(out){
						obj = JSON.parse(out);
						if(obj.ret == 1) {
							$('.user_balance').text(parseFloat((parseInt(obj.balance)) / 1000000).toFixed(6));
							$.toast({
								heading: 'Success!',
								text: obj.mes,
								showHideTransition: 'slide',
								position: 'top-right',
								icon: 'success'
							});
						}
					}
				});
			}
			
						
			$(document).ready(function() {
				$("input[type=radio][name=faucet_type]").on("change", change_faucet_type);
				show_countdown_clock(0);
				/*fetch('https://cloudflare.com/cdn-cgi/trace')
				.then(r => r.text())
				.then(text => {
					if(text.includes('warp=on')){
						$('#show_surveys').hide();
						//console.log('warp=on');
					} else {
						$('#show_surveys').show();
						//console.log('warp=off');
					}
				});*/
				//setTimeout(function(){
				
					load_deposit_transactions();
					process_complete_confirmed_request();
				//}, 500);
				
			});
			

		</script>
		
		
	
