			function show_countdown_clock(next_claim_time) {
				var clock;
				if(next_claim_time > 0) {
					$('#hourly_faucet #faucet_countdown_clock').show();
					$('#hourly_faucet #faucet_claim').hide();
					clock = new FlipClock($('.clock'), next_claim_time, {
						clockFace: 'MinuteCounter',
						autoStart: true,
						countdown: true,
						callbacks: {
							stop: function() {
								$('#hourly_faucet #faucet_claim').show();
								$('#hourly_faucet #faucet_countdown_clock').hide();
							}
						}
					});
				} else {
					$('#hourly_faucet #faucet_claim').show();
					$('#hourly_faucet #faucet_countdown_clock').hide();
				}
			}
			
			function process_claim_hourly_faucet(event) {
				$("#process_claim_hourly_faucet").attr("disabled", !0);
