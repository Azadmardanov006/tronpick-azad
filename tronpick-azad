				$.ajax({
					url: "https://tronpick.io/process.php",
					type: "POST",
					data: "action=claim_bonus_faucet",
					error: function(){
						$.toast({
							heading: 'Error!',
							text: "Request timed out. Please try again!",
							showHideTransition: 'slide',
							position: 'top-right',
							icon: 'error'
						});
						$("#process_claim_bonus_faucet").attr("disabled", false);
						$("#process_claim_bonus_faucet img").hide();
						$("#process_claim_bonus_faucet span").show();
						//$("#process_claim_bonus_faucet").text(btn_label);
					},
					success: function(out){
						let obj = JSON.parse(out);
						if(obj.ret == 0) {
							$("#process_claim_bonus_faucet").attr("disabled", false);
							$("#process_claim_bonus_faucet img").hide();
							$("#process_claim_bonus_faucet span").show();
							//$("#process_claim_bonus_faucet").text(btn_label);
