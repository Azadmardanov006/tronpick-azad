									clockFace: 'MinuteCounter',
									autoStart: true,
									countdown: true,
									callbacks: {
										stop: function() {
											$('#faucet_claim').show();
											$('#faucet_countdown_clock').hide();
										}
									}
								});
								$('.user_balance').text(parseFloat((parseInt(obj.balance)) / 1000000).toFixed(6));
								$.toast({
									heading: 'Success!',
									text: obj.mes,
									showHideTransition: 'slide',
									position: 'top-right',
									icon: 'success'
								});					
							}, 100);
						}
					}
				});
			}
			
			function process_claim_bonus_faucet() {
